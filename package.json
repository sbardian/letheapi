{
  "name": "lethe",
  "version": "1.0.0",
  "description": "A GraphQL API for lethe",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run clean",
    "test": "jest --verbose --forceExit",
    "test:watch": "jest --verbose --watch",
    "test:coverage": "npm run test -- --coverage",
    "dev": "cross-env NODE_ENV=development BABEL_DISABLE_CACHE=1 nodemon -e js,json,graphql --exec babel-node src/index.js | bunyan -o short -L",
    "lint": "eslint . ",
    "build": "npm run build:server",
    "prebuild": "npm run clean",
    "clean": "rimraf dist coverage",
    "build:server": "babel src -d dist --source-maps --ignore \"**/*.test.js\""
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Brian D. Andrews",
  "license": "MIT",
  "dependencies": {
    "apollo-engine": "1.1.1",
    "apollo-server-express": "1.3.6",
    "bcrypt": "2.0.1",
    "body-parser": "1.18.3",
    "bunyan": "1.8.12",
    "common-tags": "1.7.2",
    "compression": "1.7.2",
    "cors": "2.8.4",
    "dataloader": "1.4.0",
    "express": "4.16.3",
    "express-jwt": "5.3.1",
    "faker": "4.1.0",
    "graphql": "0.13.2",
    "graphql-tools": "3.0.2",
    "jsonwebtoken": "8.2.1",
    "mongodb-memory-server": "1.7.4",
    "mongoose": "5.1.2",
    "yargs": "11.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-jest": "22.4.4",
    "babel-plugin-inline-import": "2.0.6",
    "babel-plugin-source-map-support": "2.0.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "cross-env": "5.1.5",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.12.0",
    "jest": "22.4.4",
    "jest-environment-node": "22.4.3",
    "prettier": "1.12.1"
  }
}
