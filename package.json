{
  "name": "shopping-list",
  "version": "1.0.0",
  "description": "A GraphQL API for ToDos",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run clean",
    "test": "jest --forceExit",
    "test:watch": "jest --watch",
    "test:coverage": "npm run test -- --coverage",
    "dev": "cross-env NODE_ENV=development BABEL_DISABLE_CACHE=1 nodemon -e js,json,graphql --exec babel-node src/index.js | bunyan -o short -L",
    "lint": "eslint . ",
    "build": "npm run build:server",
    "prebuild": "npm run clean",
    "clean": "rimraf dist",
    "build:server": "babel src -d dist --source-maps --ignore \"**/*.test.js\""
  },
  "author": "Brian D. Andrews",
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "^1.3.5",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.2",
    "bunyan": "^1.8.12",
    "common-tags": "^1.7.2",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "faker": "^4.1.0",
    "graphql": "^0.13.2",
    "graphql-tools": "^2.24.0",
    "jsonwebtoken": "^8.2.1",
    "mongoose": "^5.0.15"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.2",
    "babel-eslint": "8.2.3",
    "babel-jest": "22.4.3",
    "babel-plugin-inline-import": "2.0.6",
    "babel-plugin-source-map-support": "2.0.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "cross-env": "5.1.4",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.11.0",
    "jest": "22.4.3",
    "jest-environment-node": "22.4.3",
    "prettier": "1.12.1"
  }
}
