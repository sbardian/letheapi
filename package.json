{
  "name": "letheapi",
  "version": "1.0.0",
  "description": "A GraphQL API for lethe",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "pretest": "npm run clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "npm run test -- --coverage",
    "dev": "cross-env NODE_ENV=development BABEL_DISABLE_CACHE=1 nodemon -e js,json,graphql --exec babel-node src/index.js | bunyan -o short -L",
    "lint": "eslint . ",
    "build": "npm run build:server",
    "prebuild": "npm run clean",
    "clean": "rimraf dist coverage",
    "build:server": "babel src -d dist --source-maps --ignore \"**/*.test.js\""
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Brian D. Andrews",
  "license": "MIT",
  "dependencies": {
    "apollo-engine": "1.1.2",
    "apollo-server": "2.2.0",
    "apollo-server-express": "2.2.0",
    "bcrypt": "3.0.2",
    "bunyan": "1.8.12",
    "common-tags": "1.8.0",
    "cors": "2.8.5",
    "dataloader": "1.4.0",
    "express": "4.16.4",
    "express-jwt": "5.3.1",
    "faker": "4.1.0",
    "get-stream": "4.1.0",
    "graphql": "14.0.2",
    "http": "0.0.0",
    "jsonwebtoken": "8.3.0",
    "mongodb-memory-server": "2.7.0",
    "mongoose": "5.3.11",
    "now-storage": "1.3.0",
    "subscriptions-transport-ws": "0.9.15",
    "yargs": "12.0.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-plugin-inline-import": "3.0.0",
    "babel-plugin-source-map-support": "2.0.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "cross-env": "5.2.0",
    "eslint": "5.9.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-import": "2.14.0",
    "jest": "23.6.0",
    "jest-environment-node": "23.4.0",
    "prettier": "1.15.1"
  }
}
