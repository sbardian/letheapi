{
  "name": "letheapi",
  "version": "1.0.0",
  "description": "A GraphQL API for lethe",
  "main": "index.js",
  "author": "Brian D. Andrews",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "pretest": "yarn clean",
    "test": "SL_MOCK_MODE=true jest",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "dev": "cross-env NODE_ENV=development BABEL_DISABLE_CACHE=1 npx nodemon -e js,json,graphql --exec babel-node src/index.js | bunyan -o short -L",
    "lint": "eslint . ",
    "build": "yarn build:server",
    "prebuild": "yarn clean",
    "clean": "rimraf dist coverage",
    "build:server": "babel src -d dist --source-maps --ignore \"**/*.test.js\""
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn lint"
    }
  },
  "dependencies": {
    "apollo-engine": "1.1.2",
    "apollo-server": "2.9.16",
    "apollo-server-express": "2.9.16",
    "bcrypt": "3.0.7",
    "bunyan": "1.8.12",
    "common-tags": "1.8.0",
    "cors": "2.8.5",
    "dataloader": "2.0.0",
    "express": "4.17.1",
    "express-jwt": "5.3.1",
    "faker": "4.1.0",
    "get-stream": "5.1.0",
    "graphql": "14.5.8",
    "http": "0.0.0",
    "jsonwebtoken": "8.5.1",
    "mongodb-memory-server": "6.2.3",
    "mongoose": "5.8.9",
    "now-storage": "1.3.0",
    "subscriptions-transport-ws": "0.9.16",
    "yargs": "15.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.8.3",
    "@babel/core": "7.8.3",
    "@babel/node": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "babel-eslint": "10.0.3",
    "babel-jest": "25.1.0",
    "babel-plugin-inline-import": "3.0.0",
    "babel-plugin-source-map-support": "2.1.1",
    "cross-env": "7.0.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-import": "2.20.0",
    "husky": "4.2.1",
    "jest": "25.1.0",
    "jest-environment-node": "25.1.0",
    "prettier": "1.19.1"
  }
}
